@model IEnumerable<LFbrokersV2.Models.EspecialidadPrimaPorSuma>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PrimaBase)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SumaAsegurada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EspecialidadCodigoExterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrimaVigenteDesde)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EspecialidadNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductoAseguradoraNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PrimaBase)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SumaAsegurada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EspecialidadCodigoExterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimaVigenteDesde)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EspecialidadNavigation.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductoAseguradoraNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
